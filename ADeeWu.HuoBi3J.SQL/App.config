<?xml version="1.0"?>
<configuration>
  
  <appSettings>

    <!-- ADeeWu.HuoBi3J.SQL.DataBase 基本配置 -->
    
    <!-- 设置是否启动调试模式 -->
    <add key="ADeeWu.HuoBi3J.SQL.DataBase.Debug" value="true"/>
    <!-- 设置是否启动错误追踪 -->
    <add key="ADeeWu.HuoBi3J.SQL.DataBase.ErrorTrace" value="false"/>
    <!-- 设置是否总是追踪 -->
    <add key="ADeeWu.HuoBi3J.SQL.DataBase.AlwaysTrace" value="false"/>
    <!-- 设置日志器记录日志文件路径 -->
    <add key="ADeeWu.HuoBi3J.SQL.DataBase.LogPath" value="/www/log/"/>
    <!-- 记录模式 可选值: web 与 windows . 针对LogPath 而定 -->
    <add key="ADeeWu.HuoBi3J.SQL.DataBase.LogMode" value="web"/>

    <!-- 设置数据库驱动工厂提供器 -->
    <!--可选DBProvider: MsSqlProvider , OdbcProvider , OledbProvider , MySqlProvider-->
    <add key="ADeeWu.HuoBi3J.SQL.DataBase.DBProvider" value="ADeeWu.HuoBi3J.SQL.DBProvider.MsSqlProvider"/>
    
    <!-- 设置连接字符串 -->
    <add key="ADeeWu.HuoBi3J.SQL.DataBase.ConnectionString" value="Data Source=;Initial Catalog=;User ID=;pwd=;"/>
    <add key="ADeeWu.HuoBi3J.SQL.DataBase.ConnectionString2" value="Provider=Microsoft.Jet.OLEDB.4.0;Data Source=|DataDirectory|\1.mdb;User Id=admin;Password=;"/>
    
    
    
    
    
    <!-- ADeeWu.HuoBi3J.SQL.Logger 基本配置 -->

    <!-- 设置日志器记录日志文件路径 -->
    <add key="ADeeWu.HuoBi3J.SQL.Logger.TextLogger.LogPath" value="C:\"/>
    
    
    
    
    
  </appSettings>
  
  <connectionStrings>

    
  </connectionStrings>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/></startup></configuration>
