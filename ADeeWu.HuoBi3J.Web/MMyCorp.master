                              {%>
                            <a href="/My/Corp/CashTickets/ServiceApplication.aspx">服务申请开通</a>
                            <%}
                              else
                              {%>
                            <a href="/My/Corp/CashTickets/">现金券列表</a>
                            <%} %>
                        </li>
                    </ul>
