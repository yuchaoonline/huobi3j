                            <a href="/My/Corp/Jobs/">发布职位管理</a>
                        </li>
                        <li>
                            <a href="/My/Corp/Jobs/New.aspx">发布职位</a>
                        </li>
                    </ul>

                </div>
                <div class="box_bottom"></div>
            </div>
            <%} %>

