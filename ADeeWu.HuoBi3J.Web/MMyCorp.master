<%@ Master Language="C#" MasterPageFile="~/MMain.Master" AutoEventWireup="true" CodeBehind="MMyCorp.master.cs" Inherits="ADeeWu.HuoBi3J.Web.MMyCorp" %>

<asp:Content ID="Content3" ContentPlaceHolderID="cphTitle" runat="server">
